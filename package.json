{
  "name": "combell",
  "version": "0.1.4",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "license": "MIT",
  "directories": {
    "test": "./test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "combell": "node ./dist/index.js",
    "start": "yarn run nodemon",
    "nodemon": "nodemon",
    "build-backup": "NODE_ENV=production webpack && bundlesize",
    "release": "gulp scripts",
    "test": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js --reporter lcov --require babel-core/register ./node_modules/.bin/_mocha \"./test/**/*.spec.js\"",
    "coverage": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js report --reporter=text-lcov",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^5.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "bundlesize": "^0.17.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-eslint": "^4.0.2",
    "mocha": "^5.2.0",
    "nock": "^9.2.6",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "rewire": "^4.0.1",
    "rollup": "^0.60.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-license": "^0.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1",
    "sinon": "^5.0.10"
  },
  "bundlesize": [
    {
      "path": "./dist/combell.min.js",
      "threshold": "5kB"
    }
  ]
}
